#include <bits/stdc++.h>

using namespace std;
int arr[2000000];
int main()
{
    ios_base::sync_with_stdio(0);
    cin.tie(0);
    cout.tie(0);
    int x ;
    cin >> x;
    while(x != 0)
    {
        for(int i = 0 ; i < x ; i++)
            cin >> arr[i];
        sort(arr,arr+x);
        for(int i = 0 ; i < x ; i++)
        {
            cout<<arr[i];
            if(i != x-1)
                cout<<" ";
        }
        cout<<endl;
        cin >> x;

    }
    return 0;
}
